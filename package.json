{
  "name": "natours",
  "private": true,
  "version": "1.0.0",
  "description": "Server for natours",
  "main": "server.js",
  "scripts": {
    "start": "node server.js NODE_ENV=production",
    "dev": "nodemon -e js,json,pug server.js NODE_ENV=development",
    "prod": "nodemon server.js NODE_ENV=production",
    "debug": "ndb server.js NODE_ENV=development",
    "import-data": "node dev-data/data/script.js",
    "watch:js": "parcel watch ./public/js/index.js -d ./public",
    "build:js": "parcel build ./public/js/index.js -d ./public",
    "refresh-db": "yarn import-data --delete all && yarn import-data --import all"
  },
  "author": "Siddharth Borderwala",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.1",
    "hpp": "^0.2.3",
    "html-to-text": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.13",
    "pug": "^3.0.0",
    "sharp": "^0.26.2",
    "slugify": "^1.4.0",
    "stripe": "^8.121.0",
    "validator": "^13.1.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": "14.15.0",
    "yarn": "1.22.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
